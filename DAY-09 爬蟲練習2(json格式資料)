items=result[0]["data"]["extendedFeedItems"]
for item in items:
    print(item["post"]["title"])
# Â∞áË≥áÊñôÂØ´ÂÖ• data3.txt Ê™îÊ°à‰∏≠
with open("data3.txt","w", encoding="utf-8") as file:
    for item in items:
        file.write(item["post"]["title"]+"\n")
        print(item["post"]["title"])
