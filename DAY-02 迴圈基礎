# for 迴圈
for x in [3,5,1]:
    print(x)
for x in "hello":
    print(x)
for x in range(5): # 相當於 01234 的列表
    print(x)
for x in range(5,10): # 相當於 56789 的列表
    print(x)

# 1+2+3+....+10 用 for 迴圈完成
sum=0
for i in range(11):
    sum=sum+i
    i+=1
print(sum)

# while 迴圈 1+2+3+...+10
n=1
sum=0 # 紀錄累加結果
while n<=10:
    sum=sum+n
    n+=1
print(sum)
